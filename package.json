{
  "name": "ios-chatbot",
  "version": "1.0.0",
  "description": "AI Chatbot Interface",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "test": "jest",
    "build": "npm run build:client && npm run build:server",
    "build:client": "mkdir -p dist && cp -R public/* dist/ && cp index.html dist/",
    "build:server": "babel app.js -d dist",
    "deploy": "gh-pages -d dist"
  },
  "keywords": [
    "chatbot",
    "AI",
    "interface"
  ],
  "author": "",
  "license": "ISC",
  "nodemonConfig": {
    "watch": [
      "app.js",
      "public"
    ],
    "ext": "js,json,html,css"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csrf": "^3.1.0",
    "dompurify": "^2.3.3",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-session": "^1.17.3",
    "helmet": "^6.0.1",
    "marked": "^4.0.0",
    "morgan": "^1.10.0",
    "socket.io": "^4.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "gh-pages": "^3.2.3",
    "jest": "^27.0.0",
    "nodemon": "^3.1.7"
  }
}
